export function levelArray(level) {
    const levels = {
        1 : [1, 2, 3, 4, 5],
        2 : [1, 4, 3, 2, 5],
        3 : [2, 1, 5, 4, 3],
        4 : [3, 4, 2, 5, 1],
        5 : [5, 4, 3, 2, 1]
    }
    return levels[level];
}


export const getRandomNumber = (min, max) => {
    return Math.floor(Math.random() * (max - min + 1)) + min;
}